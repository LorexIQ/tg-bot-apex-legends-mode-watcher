{
  "name": "node-ts-app",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "keywords": [],
  "main": ".engine/index.js",
  "_moduleAliases": {
    "@": "dist/src",
    "@composables": "dist/.engine/composables",
    "@root": "dist/.engine/root"
  },
  "scripts": {
    "start": "node ./scripts/start.js",
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "build:clean": "rimraf dist",
    "db:mig": "node ./scripts/db.js prisma migrate dev",
    "db:mig:reset": "node ./scripts/db.js prisma migrate reset",
    "db:gen": "node ./scripts/db.js prisma generate",
    "db:push": "node ./scripts/db.js prisma db push",
    "db:push:force": "node ./scripts/db.js prisma db push --force-reset",
    "db:seed": "prisma db seed",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "prisma": {
    "seed": "node ./scripts/db.js ts-node ./prisma/seed.ts"
  },
  "dependencies": {
    "@grammyjs/hydrate": "^1.4.1",
    "@grammyjs/menu": "^1.3.0",
    "@grammyjs/storage-prisma": "^2.5.1",
    "@prisma/client": "^6.5.0",
    "axios": "^1.8.4",
    "cron": "^4.1.3",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "grammy": "^1.35.0",
    "log4js": "^6.9.1",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.6",
    "reflect-metadata": "^0.2.2",
    "tsx": "^4.19.3",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.1",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^22.13.11",
    "eslint": "^9.10.0",
    "prisma": "^6.5.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.8.2"
  }
}
